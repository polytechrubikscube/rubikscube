#include "rubikscube.h"

t_cube	*ft_read_file(char *path)
{
	int fd;
	ssize_t size;
	char *buff

	fd = open(path, O_RDONLY);
	if (fd == -1)
	{
		print_f("File Reading Error\n");
		return (NULL);
	}

	while ((size = read(fd, buff, BUFF_SIZE) > 0)
	{
		
	}	
} 
